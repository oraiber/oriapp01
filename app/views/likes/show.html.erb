  <div class="likes_list" >
  <p class="mypage_list"><%= link_to "#{@nickname}さんの投稿一覧へ", user_path(current_user.id), method: :get %></p>
    <p><%= @nickname %>さんのいいね一覧</p>
    <% if @like_list.present? %>
      <% @like_list.each do |tweet| %>
      <div class = "box">
        <%= tweet.user.nickname %>
        <p><%= link_to safe_join(tweet.text.split("\n"),tag(:br)), tweet_path(tweet.id) %></p>
      </div>
      <% end %>
    <% end %>
  </div>